  "components": {
    "schemas": {
      "LoginRequest": {
        "type": "object",
        "required": ["email", "password"],
        "properties": {
          "email": {
            "type": "string",
            "format": "email",
            "description": "Email đăng nhập",
            "example": "string"
          },
          "password": {
            "type": "string",
            "format": "password",
            "description": "Mật khẩu",
            "example": "string"
          }
        }
      },
      "LoginResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "enum": ["success"],
            "example": "string"
          },
          "token": {
            "type": "string",
            "description": "JWT access token",
            "example": "string"
          },
          "user": {
            "$ref": "#/components/schemas/User"
          }
        }
      },
      "RegisterRequest": {
        "type": "object",
        "required": ["fullName", "phone", "email", "password"],
        "properties": {
          "fullName": {
            "type": "string",
            "description": "Họ và tên",
            "example": "string"
          },
          "phone": {
            "type": "string",
            "pattern": "^0[0-9]{9,10}$",
            "description": "Số điện thoại",
            "example": "string"
          },
          "email": {
            "type": "string",
            "format": "email",
            "description": "Email",
            "example": "string"
          },
          "password": {
            "type": "string",
            "format": "password",
            "minLength": 6,
            "description": "Mật khẩu (tối thiểu 6 ký tự)",
            "example": "string"
          }
        }
      },
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "example": "string"
          },
          "fullName": {
            "type": "string",
            "example": "string"
          },
          "email": {
            "type": "string",
            "format": "email",
            "example": "string"
          },
          "phone": {
            "type": "string",
            "example": "string"
          },
          "role": {
            "type": "string",
            "enum": ["Student", "Event Organizer", "Staff"],
            "description": "Vai trò người dùng",
            "example": "string"
          },
          "status": {
            "type": "string",
            "enum": ["ACTIVE", "INACTIVE", "BLOCKED"],
            "description": "Trạng thái tài khoản",
            "example": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time",
            "example": "string"
          }
        }
      },
      "ErrorResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "enum": ["fail", "error"],
            "example": "string"
          },
          "code": {
            "type": "string",
            "description": "Mã lỗi",
            "example": "string"
          },
          "message": {
            "type": "string",
            "description": "Mô tả lỗi",
            "example": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "description": "JWT token được trả về từ API login. Nhập vào ô bên dưới với format: `Bearer {token}`"
      }
    }
  },
  "security": []
}
